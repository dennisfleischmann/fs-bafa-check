{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "OfferFacts",
  "type": "object",
  "required": [
    "case_id",
    "building",
    "applicant",
    "offer",
    "docs"
  ],
  "properties": {
    "case_id": {
      "type": "string"
    },
    "building": {
      "type": "object"
    },
    "applicant": {
      "type": "object"
    },
    "offer": {
      "type": "object",
      "required": [
        "measures"
      ],
      "properties": {
        "measures": {
          "type": "array"
        }
      }
    },
    "docs": {
      "type": "object"
    }
  }
}
